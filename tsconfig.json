{
    "compilerOptions": {
      "jsx": "react-jsx",
      "target": "ES2020",
      "module": "ESNext",
      "strict": true,
      "skipLibCheck": true,
      "moduleResolution": "bundler",
      "allowImportingTsExtensions": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["src"]
  }
  